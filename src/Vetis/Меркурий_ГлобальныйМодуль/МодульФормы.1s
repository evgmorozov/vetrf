			Если ПолучитьКонстанту("ВыводитьПодробнуюИнформацию")=1 Тогда  
				RS.Отладка(1);
			КонецЕсли;		
			ТЗ = RS.ВыполнитьИнструкцию(ТекстЗапроса);
	  	
	Иначе	
			
