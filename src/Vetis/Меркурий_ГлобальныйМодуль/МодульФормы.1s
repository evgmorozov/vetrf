Функция Отправить_ВСД_Аннулирование( док ) Экспорт
	
	t1 = Старт();
	
	ЗапросXML = ВСД_Аннулирование_ЗапросXML(док);

	appID = ОтправитьТранзакцию( ЗапросXML , док);

	Финиш(t1, Сам(), "Отправить_ВСД_Аннулирование", док);
	
	Возврат appID;
КонецФункции


Процедура Аннулировать_ВСД_транзакция( док ) Экспорт

	тзВСД = Выбрать_ВСД( док );

	тзВСД.ВыбратьСтроки();
	Пока тзВСД.ПолучитьСтроку()=1 Цикл
		Отправить_ВСД_Аннулирование(тзВСД.Док);		
		Док_ВСД_транзакция = тзВСД.Док.ДокОснование;
		Статус = тзВСД.Док.Статус;
	КонецЦикла;
	
	Если тзВСД.Док.СтатусВСД=Перечисление.ВСД_СтатусВСД.WITHDRAWN Тогда 
		докОбъект = СоздатьОбъект("Документ");
		докОбъект.НайтиДокумент( Док_ВСД_транзакция );
		докОбъект.Статус = Статус;
		докОбъект.СтатусВСД = Перечисление.ВСД_СтатусВСД.WITHDRAWN;
		докОбъект.Записать();
		докОбъект.Удалить(0);
	КонецЕсли;

КонецПроцедуры

Процедура СоздатьХС( ВыбЗначение ) Экспорт

	СписокПараметров = СоздатьОбъект("СписокЗначений");
	Если ТипЗначенияСтр(ВыбЗначение) = "ТаблицаЗначений" Тогда 
		СписокПараметров.ДобавитьЗначение( ВыбЗначение.ХозСубъект, "ХС");
	Иначе
		СписокПараметров.ДобавитьЗначение( ВыбЗначение, "ХС" );
	КонецЕсли;

	ОткрытьФорму("Отчет", СписокПараметров, ПолучитьКонстанту("КаталогМодуля")+"ВСД_СоздатьХС.ert");

КонецПроцедуры

