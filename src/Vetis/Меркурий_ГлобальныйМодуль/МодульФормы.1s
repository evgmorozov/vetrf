			ТекстЗапроса = "select
			|	СпрПл.ID [id $Справочник.ВСД_Площадка],
			|	СпрПл.descr,
			|	СпрПл.GUID,
			|	СпрПл.Контрагент
			|from [Справочник.ВСД_Площадка] as СпрПл
			|where
			|	СпрПл.Контрагент=:ВыбКлиент
			|";

			Запрос.Подставлять("ВыбКлиент", ВыбКлиент);
	
			Если ПолучитьКонстанту("ВыводитьПодробнуюИнформацию")=1 Тогда  
