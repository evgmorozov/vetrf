	КонецЕсли;

	Сообщить("Отправляем запрос результата ["+applicationID+"] " );
	Результат = ПолучитьРезультатСервер( applicationID );

    Сообщить("Загрузка XML-файла: "+ Компонента.LogFilename);
    objDom=СоздатьОбъект("MSXML2.DOMDocument");
