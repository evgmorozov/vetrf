	//appID = ОтправитьЗапросXML(ЗапросXML);
	appID = ОтправитьЗапросSoap(ЗапросXML, метод);

	Сам().Пауза();
	ХозСубъект_Создать_Результат(appID, ВыбХозСубъект );

	Возврат appID;
