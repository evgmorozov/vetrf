			ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "%УсловиеПоСкладу%", УсловиеПоСкладу);

			RS.УстановитьТекстовыйПараметр("Отправитель_ХозСубъект", ПолучитьКонстанту("Отправитель_ХозСубъект") );
